{
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "flowtype"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "arrow-body-style": "off",
      "arrow-parens": "off"
    }
}
